#import <UIKit/UIKit.h>

@class HETAuthorizeWebView;
@protocol AuthorizeDialogDelegate <NSObject>
- (void)AuthorizeDialogLogin:(NSString*)code;

@optional
- (void)AuthorizeDialogNotLogin:(BOOL)cancel;

- (void)dialogDidComplete:(HETAuthorizeWebView *)dialog;
- (void)dialogDidNotComplete:(HETAuthorizeWebView *)dialog;
- (void)dialogCompleteWithUrl:(NSURL *)url;
- (void)dialogDidNotCompleteWithUrl:(NSURL *)url;
- (void)dialog:(HETAuthorizeWebView*)dialog didFailWithError:(NSError *)error;

@end
@interface HETAuthorizeWebView : UIView<UIWebViewDelegate>
@property (nonatomic, strong) NSURLCredential *credential;

-(id) initWithURL:(NSURL *) authorizeURL
         delegate:(id <AuthorizeDialogDelegate>) delegate;
- (void)show;

@end
