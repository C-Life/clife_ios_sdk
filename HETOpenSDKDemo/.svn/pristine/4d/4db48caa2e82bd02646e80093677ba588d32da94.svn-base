#import "bindJavaScript.h"

@implementation bindJavaScript

-(void)yes
{
    /*if(self.deviceControlBlock)
    {
        self.deviceControlBlock(@"yes");
    }*/
    if(self.bindJavaScriptdelegate &&[self.bindJavaScriptdelegate respondsToSelector:@selector(yesButton)])
    {
        [self.bindJavaScriptdelegate yesButton];
    }
}
-(void)config:(id)data
{
    
    if(self.configBlock)
    {
        self.configBlock(data);
    }
}

-(void)send:(id)data :(id)successCallback :(id)errorCallback
{
    if(self.deviceControlBlock)
    {
        self.deviceControlBlock(data,successCallback,errorCallback);
    }

}
-(void)setTitle:(id)data
{
    if(self.getTitleBlock)
    {
        self.getTitleBlock(data);
    }
}


-(void)tips:(id)data
{
    if(self.gettipsBlock)
    {
        self.gettipsBlock(data);
    }
}
@end
