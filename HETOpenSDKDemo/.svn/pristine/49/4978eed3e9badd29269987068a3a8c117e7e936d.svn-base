#import "HETAccount.h"

@implementation HETAccount

- (id)init {
    self = [super init];
    return self;
}


//===========================================================
//  Keyed Archiving
//
//===========================================================
- (void)encodeWithCoder:(NSCoder *)encoder
{

    [encoder encodeObject:self.accessToken forKey:@"openId"];
    [encoder encodeObject:self.accessToken forKey:@"accessToken"];
    [encoder encodeObject:self.accessToken forKey:@"refreshToken"];
    [encoder encodeObject:self.expirationDate forKey:@"expirationDate"];
    [encoder encodeBool:self.selected forKey:@"selected"];
    [encoder encodeObject:self.userKey forKey:@"userKey"];
}

- (id)initWithCoder:(NSCoder *)decoder
{
    self = [super init];
    if (self) {

        self.openId = [decoder decodeObjectForKey:@"openId"];
        self.accessToken = [decoder decodeObjectForKey:@"accessToken"];
        self.refreshToken = [decoder decodeObjectForKey:@"refreshToken"];
        self.expirationDate = [decoder decodeObjectForKey:@"expirationDate"];
        self.selected = [decoder decodeBoolForKey:@"selected"];
        self.userKey=[decoder decodeObjectForKey:@"userKey"];
    }
    return self;
}

@end
